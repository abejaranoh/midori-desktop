{
  "name": "AstianGO",
  "description": "Astian Search",
  "manifest_version": 2,
  "version": "1.1",
  "applications": {
    "gecko": {
      "id": "astiango@search.mozilla.org"
    }
  },
  "hidden": true,
  "icons": {
    "16": "favicon.png"
  },
  "web_accessible_resources": ["favicon.ico"],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "AstianGO",
      "search_url": "https://astiango.co/q={searchTerms}",
      "suggest_url": "https://ac.duckduckgo.com/ac/",
      "suggest_url_get_params": "q={searchTerms}&type=list"
    }
  }
}